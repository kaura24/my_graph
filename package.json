    "flexsearch": "^0.8.212",
    "graphology": "^0.26.0",
    "html-to-text": "^9.0.5",
    "lucide-react": "^0.575.0",
    "markdown-it": "^14.1.0",
    "openai": "^5.20.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "reactflow": "^11.11.4",
