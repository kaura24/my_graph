  "type": "module",
  "scripts": {
    "dev": "node scripts/start-backend.cjs",
    "dev:frontend": "vite",
    "dev:backend": "cd python && .venv/Scripts/python.exe -m uvicorn app:app --host 127.0.0.1 --port 8000 --reload",
